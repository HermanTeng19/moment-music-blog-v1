{
  "version": 2,
  "name": "moment-music-blog",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(mp3|jpg|png|svg|txt|json))",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/audio/(.*)",
      "headers": [
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        },
        {
          "key": "Content-Type", 
          "value": "audio/mpeg"
        }
      ]
    }
  ]
}
